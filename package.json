    "vide-plugin-context-css": "*",
    "vide-plugin-context-js": "*",
    "vide-plugin-context-html": "*",
    "babel-traverse": "^6.24.1",
    "babylon": "^6.17.1",
    "vue-template-compiler": "2.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "simple-file-logger": "1.0.1"
  }
