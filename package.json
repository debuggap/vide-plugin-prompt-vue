{
  "name": "vide-plugin-prompt-vue",
  "version": "0.1.0",
  "author": "vic.wang",
  "scripts": {
    "start": "babel ./src/ -d ./lib/"
  },
  "vide": {
    "type": "prompt",
    "promptExtension": [
      "vue"
    ],
    "promptPriority": 1
  },
  "keywords": [
    "vide-plugin",
